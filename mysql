  `key2` char(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `key3` char(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `key4` char(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `key5` char(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `key6` char(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `key7` char(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `key8` char(255) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
